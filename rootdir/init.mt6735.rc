service qmc6983d /system/bin/qmc6983d
    disabled
    user system
    group system
    class main
    oneshot
    
service mpud6050 /system/bin/mpud6050
    oneshot
    user system
    group system

service mc64xxd /system/bin/mc64xxd
    disabled
    user system
    group system
    class main
