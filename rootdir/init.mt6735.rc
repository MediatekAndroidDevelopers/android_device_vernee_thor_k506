service wifi2agps /system/bin/wifi2agps
    class main
    user wifi
    group gps inet

service mnld /system/bin/mnld
    class main
    user gps
    group gps inet misc sdcard_rw sdcard_r media_rw system
    socket mnld stream 660 gps system

